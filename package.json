{
  "name": "next-pwa-babily",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "resetsw-and-lauch": "rm public/sw.js && rm public/workbox-* ; next build && next start",
    "lint": "eslint .",
    "lint:staged": "npx lint-staged",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md|vue)\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all lint:staged lint"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md|vue)\"",
      "eslint . --fix"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "axios": "^0.21.1",
    "imagemin": "^7.0.1",
    "imagemin-webp": "^6.0.0",
    "next": "10.0.3",
    "next-google-fonts": "^1.2.1",
    "next-pwa": "^5.0.0",
    "nextjs-progressbar": "0.0.7",
    "react": "17.0.1",
    "react-burger-menu": "^3.0.2",
    "react-calendly": "^1.3.0",
    "react-dom": "17.0.1",
    "react-google-recaptcha": "^2.1.0",
    "react-hook-form": "^6.14.2",
    "react-icons": "^4.1.0",
    "react-responsive": "^8.2.0",
    "react-spring": "^8.0.27"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.12.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "typescript": "^4.1.2"
  }
}
